// promediosde3numeros.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include "conio.h"
#include <iostream>

using namespace std;


void main(){
	float a,b,c,promedio;
	cout<<"Ingrese el primer numero: ";
	cin>>a;
	cout<<"Ingrese el segundo numero: ";
	cin>>b;
	cout<<"Ingrese el tercer numero: ";
	cin>>c;

	promedio=  (a+b+c)/3;

	cout<<"El promedio de los tres numeros son: "<<promedio;

	getch();
}
